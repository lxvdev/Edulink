    <system:String x:Key="SendMessageDialogContent">Jaką wiadomość chciał(a)byś wysłać?</system:String>
    <system:String x:Key="SendMessageDialogCaption">Wyślij wiadomość</system:String>
    <!--About window-->
    <system:String x:Key="About">O programie</system:String>
