<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Author">@.etsun0</system:String>
    <!--Settings window-->
    <system:String x:Key="SettingsTitle">Ustawienia</system:String>
    <system:String x:Key="NameTitle">Nazwa</system:String> <!--Need to update this-->
    <system:String x:Key="IPAddressTitle">Adres IP</system:String>
    <system:String x:Key="PortTitle">Port</system:String>
