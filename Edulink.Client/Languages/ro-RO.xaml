    <system:String x:Key="WelcomeText">Bine ai venit la Edulink!</system:String>
    <system:String x:Key="BreifDescriptionText">Pentru a te conecta la computerul profesorului, te rugăm să configureze adresa IP (găsită în colțul din dreapta sus al aplicației server EduLink) portul și numele computerului pentru identificare.</system:String>
    <system:String x:Key="ChangeLaterText">Poți modifica aceste setări mai târziu din zona de pictograme.</system:String>
    <system:String x:Key="DoneButton">Finalizare</system:String>
    <!--Dialog input-->
    <system:String x:Key="SendMessageDialogContent">Ce mesaj vrei să trimiți?</system:String>
    <system:String x:Key="SendMessageDialogCaption">Trimite mesaj</system:String>
