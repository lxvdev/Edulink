<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Author">My mom</system:String>
    <!--Settings window-->
    <system:String x:Key="SettingsTitle">Setări</system:String>
    <system:String x:Key="NameTitle">Nume</system:String> <!--Need to update this-->
    <system:String x:Key="IPAddressTitle">Adresă IP</system:String>
    <system:String x:Key="PortTitle">Port</system:String>
    <system:String x:Key="MiscellaneousTitle">Diverse</system:String>
    <system:String x:Key="LanguageTitle">Limbă</system:String>
    <system:String x:Key="RestartAppButton">Reporniți aplicația</system:String>
    <system:String x:Key="ResetSettingsButton">Resetează setările</system:String>
    <system:String x:Key="ExitButton">Închideți aplicația</system:String>
    <system:String x:Key="SaveButton">Salvează setări</system:String>
    <system:String x:Key="SaveAndRestartButton">Salvează și reportește aplicația</system:String>
    <!--Message window-->
    <system:String x:Key="MessageTitleBar">Mesaj de la profesor</system:String>
    <system:String x:Key="OkButton">Confirmare</system:String>
